#This file contains the function prototypes and
#annotations for arguments. The .h and .c files
#for the functions defined will be generated 
#using the boiler plate generation script.
#Arguments annotated with the keyword "read"
#must be read and captured.


#Most commonly used read, write and close functions
int close(read int fd);
int creat(read can const char *pathname, read mode_t mode);
int creat64(read can const char *pathname, read mode_t mode);
int fclose(read FILE *fp);
int fcloseall(void);
FILE *fdopen(read int fd, read const char *mode);
int fgetc(read FILE *stream);
char *fgets(char *s, read int size, read FILE *stream);
FILE *fopen(read can const char *path, read const char *mode);
FILE *fopen64(read can const char *path, read const char *mode);
int fputc(int c, read FILE *stream);
int fputs(const char *s, read FILE *stream);
size_t fread(void *ptr, read size_t size, read size_t nmemb, read FILE *stream);
FILE *freopen(read can const char *path, read const char *mode, read FILE *stream);
FILE *freopen64(read can const char *filename, read const char *type, read FILE *stream);
size_t fwrite(const void *ptr, read size_t size, read size_t nmemb, read FILE *stream);
int getc(read FILE *stream);
int getchar(void);
__ssize_t getline(char **lineptr, size_t *n, read FILE *stream);
int putc(int c, read FILE *stream);
int putchar(int c);
int puts(const char *s);
ssize_t pread(read int fd, void *buf, read size_t count, read off_t offset);
ssize_t pwrite(read int fd, const void *buf, read size_t count, read off_t offset);
ssize_t read(read int fd, void *buf, read size_t count);
int vfprintf(read FILE *stream, const char *format, va_list ap);
int vprintf(const char *format, va_list ap);
int vscanf(const char *format, va_list ap);
int vfscanf(read FILE *stream, const char *format, va_list ap);
ssize_t write(read int fd, const void *buf, read size_t count);

#ISOc99 Scanf
int __isoc99_vscanf(const char *format, va_list ap);
int __isoc99_vfscanf(read FILE *stream, const char *format, va_list ap);

#Unlocked IO functions
int getc_unlocked(read FILE *stream);
int getchar_unlocked(void);
int putc_unlocked(int c, read FILE *stream);
int putchar_unlocked(int c);
int fgetc_unlocked(read FILE *stream);
int fputc_unlocked(int c, read FILE *stream);
size_t fread_unlocked(void *ptr, read size_t size, read size_t n, read FILE *stream);
size_t fwrite_unlocked(const void *ptr, read size_t size, read size_t n, read FILE *stream);
char *fgets_unlocked(char *s, int n, read FILE *stream);
int fputs_unlocked(const char *s, read FILE *stream);

#File pointer position related functions
int fseek(read FILE *stream, read long offset, read int whence);
int fseeko(read FILE *stream, read off_t offset, read int whence);
int fsetpos(read FILE *stream, const fpos_t *pos);
int fsetpos64(read FILE *stream, const fpos64_t *pos);
off_t lseek(read int fd, read off_t offset, read int whence);
off64_t lseek64(read int fd, read off64_t offset, read int whence);
void rewind(read FILE *stream);

#File permission related functions
int chmod(read can const char *path, read mode_t mode);
int chown(read can const char *path, read uid_t owner, read gid_t group);
int fchmod(read int fd, read mode_t mode);
int fchmodat(read int dirfd, read can const char *pathname, read mode_t mode, read int flags);
int fchown(read int fd, read uid_t owner, read gid_t group);
int fchownat(read int dirfd, read can const char *pathname, read uid_t owner, read gid_t group, read int flags);
int lchown(read abs const char *path, read uid_t owner, read gid_t group);

#Other important file related functions
int dup(read int oldfd);
int dup2(read int oldfd, read int newfd);
int dup3(read int oldfd, read int newfd, read int flags);
int ftruncate(read int fd, read off_t length);
int ftruncate64(read int fd, read off64_t length);
int link(read abs const char *path1, read can const char *path2);
int mkdir(read can const char *pathname, read mode_t mode);
int mkfifo(read can const char *pathname, read mode_t mode);
int remove(read abs const char *pathname);
int rename(read abs const char *oldpath, read can const char *newpath);
int rmdir(read can const char *pathname);
int symlink(read can const char *oldpath, read abs const char *newpath);
int system(read const char *command);
int truncate(read can const char *path, read off_t length);
int truncate64(read can const char *path, read off64_t length);
int unlink(read abs const char *pathname);
mode_t umask(read mode_t mask);
int utime(read can const char *filename, const struct utimbuf *times);

#P* commands
FILE *popen(read const char *command, read const char *type);
int pclose(read FILE *stream);

#Tempfile Creation
int mkstemp(read can char *templ);
int mkostemp(read can char *templ, read int flags);
int mkstemps(read can char *templ, int suffixlen);
int mkostemps(read can char *templ, int suffixlen, read int flags);
FILE *tmpfile(void);
FILE *tmpfile64(void);

#Stat Functions
int __fxstat(int __ver, read int fd, struct stat *buf);
int __fxstat64(int __ver, read int fd, struct stat64 *buf);
int __lxstat(int __ver, read abs const char *path, struct stat *buf);
int __lxstat64(int __ver, read abs const char *path, struct stat64 *buf);
int __xstat(int __ver, read can const char *path, struct stat *buf);
int __xstat64(int __ver, read can const char *path, struct stat64 *buf);

#Directory related functions

#Process attributes related functions
int chdir(read can const char *path);
int fchdir(read int fd);
int killpg(read int pgrp, read int sig);
int seteuid(read uid_t euid);
int setegid(read gid_t egid);
int setgid(read gid_t gid);
int setreuid(read uid_t ruid, read uid_t euid);
int setregid(read gid_t rgid, read gid_t egid);
int setuid(read uid_t uid);

#Environment functions
int clearenv(void);
int putenv(read char *string);
int setenv(read const char *name, read const char *value, read int overwrite);
int unsetenv(read const char *name);

#Manually defined function
nogen int open(const char *pathname, int flags, ...);
nogen int open64(const char *pathname, int flags, ...);
nogen int printf(const char *format, ...);
nogen int scanf(const char *format, ...);
nogen int fprintf(FILE *stream, const char *format, ...);
nogen int fscanf(FILE *stream, const char *format, ...);
nogen int __isoc99_scanf(const char *format, ...);
nogen int __isoc99_fscanf(FILE *stream, const char *format, ...);
nogen int execl(const char *path, const char *arg, ...);
nogen int execlp(const char *file, const char *arg, ...);
nogen int execle(const char *path, const char *arg);
nogen int execv(const char *path, char *const argv[]);
nogen int execvp(const char *file, char *const argv[]);
nogen int execvpe(const char *file, char *const argv[], char *const envp[]);
nogen int execve(const char *filename, char *const argv[], char *const envp[]);
nogen int fexecve(int fd, char *const argv[], char *const envp[]);
nogen pid_t fork(void);
nogen void* dlopen(const char * filename, int flag);
nogen sighandler_t signal(int signum, sighandler_t real_handler);
nogen int sigaction(int signum, const struct sigaction *act, struct sigaction *oldact);
nogen void _exit(int status);
nogen void _Exit(int status);
nogen int pthread_create(pthread_t *thread, const pthread_attr_t *attr, void *(*start_routine) (void *), void *arg);
nogen void pthread_exit(void *retval);
nogen sighandler_t sigset(int sig, sighandler_t disp);
nogen int sigignore(int sig);
